    482          
    483              // Look for the next timeout timer
    484              while ( srchTimer != NULL )
    485              {
    486                if (srchTimer->timeout < nextTimeout)
    487                {
    488                  nextTimeout = srchTimer->timeout;
    489                }
    490                // Check next timer
    491                srchTimer = srchTimer->next;
    492              }
    493            }
    494            else
    495            {
    496              // No timers
    497              nextTimeout = 0;
    498            }

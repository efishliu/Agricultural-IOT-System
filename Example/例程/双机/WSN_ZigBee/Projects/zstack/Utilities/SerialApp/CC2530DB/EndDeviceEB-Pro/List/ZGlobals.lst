    381          /*********************************************************************
    382           * @fn       zgItemInit()
    383           *
    384           * @brief
    385           *
    386           *   Initialize a global item. If the item doesn't exist in NV memory,
    387           *   write the system default (value passed in) into NV memory. But if
    388           *   it exists, set the item to the value stored in NV memory.
    389           *
    390           *   Also, if setDefault is TRUE and the item exists, we will write
    391           *   the default value to NV space.
    392           *
    393           * @param   id - item id
    394           * @param   len - item len
    395           * @param   buf - pointer to the item
    396           * @param   setDefault - TRUE to set default, not read
    397           *
    398           * @return  ZSUCCESS if successful, NV_ITEM_UNINIT if item did not
    399           *          exist in NV, NV_OPER_FAILED if failure.
    400           */
    401          static uint8 zgItemInit( uint16 id, uint16 len, void *buf, uint8 setDefault )
    402          {
    403          
    404            uint8 status;
    405          
    406            // If the item doesn't exist in NV memory, create and initialize
    407            // it with the value passed in.
    408            status = osal_nv_item_init( id, len, buf );
    409            if ( status == ZSUCCESS )
    410            {
    411              if ( setDefault )
    412              {
    413                // Write the default value back to NV
    414                status =  osal_nv_write( id, 0, len, buf );
    415              }
    416              else
    417              {
    418                // The item exists in NV memory, read it from NV memory
    419                status = osal_nv_read( id, 0, len, buf );
    420              }
    421            }
    422          
    423            return (status);
    424          }

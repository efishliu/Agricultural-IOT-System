    234          /*********************************************************************
    235          *
    236          *       _SetLineColor
    237          */
    238          static int _SetLineColor(int i) {
    239            switch (GUI_Context.LineStyle) {
    240            case GUI_LS_DASH:
    241              i = (i+6)%16;
    242              return (i<12);
    243            case GUI_LS_DOT:
    244              i %= 4;
    245              return (i<2);
    246            case GUI_LS_DASHDOT:
    247              i %= 20;
    248              if (i<12)
    249                return 1;
    250              else if ((i>=16) && (i<18))
    251                return 1;
    252              return 0;
    253            case GUI_LS_DASHDOTDOT:
    254              i %= 24;
    255              if (i<12)
    256                return 1;
    257              else if ((i>=16) && (i<18))
    258                return 1;
    259              else if ((i>=20) && (i<22))
    260                return 1;
    261              else 
    262                return 0;
    263            }
    264            return 0;
    265          }

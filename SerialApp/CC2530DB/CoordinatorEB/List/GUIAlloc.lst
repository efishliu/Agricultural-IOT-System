    475          void* GUI_ALLOC_h2p(GUI_HMEM  hMem) {
    476            U8* p = (U8*)GUI_ALLOC_H2P(hMem);    /* Pointer to memory block from memory manager */
    477            p += sizeof(INFO);                   /* Convert to pointer to usable area */
    478            return p;

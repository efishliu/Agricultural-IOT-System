    181          /*********************************************************************
    182          *
    183          *       LCD_L0_MAG_DrawHLine
    184          */
    185          void LCD_L0_MAG_DrawHLine(int x0, int y0,  int x1) {
    186            x0 *= LCD_XMAG;
    187            x1 = x1*(LCD_XMAG) + LCD_XMAG-1;
    188            y0 *= LCD_YMAG;
    189            LCD_L0_FillRect(x0, y0, x1, y0 + LCD_YMAG-1);
    190          }

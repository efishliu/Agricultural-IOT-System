    142          *
    143          *       _Flush
    144          *
    145          *  This function draw lines between points in the array
    146          */
    147          static void _Flush(int x0, int y) {
    148            int i, x = 0;
    149            char On=0;
    150            for (i=0; i<GL_FP_Cnt; i++) {
    151              int xNew = _aX[i];
    152              if (On) {
    153                LCD_HL_DrawHLine(x0 + x, y, x0 + xNew);
    154              }
    155              On ^= 1;
    156              x = xNew;
    157            }
    158          }

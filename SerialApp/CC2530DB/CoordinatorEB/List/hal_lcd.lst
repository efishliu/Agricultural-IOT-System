    118          /* clear the received and transmit byte status, write tx data to buffer, wait till transmit done */
    119          #define LCD_SPI_TX(x)                   { U1CSR &= ~(BV(2) | BV(1)); U1DBUF = x; while( !(U1CSR & BV(1)) ); }
